{
    "AppName": "BotServer",
    "DatabaseConnection": {
        "Server": "DATABASE.SERVER",
        "Port": "DATABASE.PORT",
        "Database": "DATABASE.NAME",
        "Username": "DATABASE.USERNAME",
        "Password": "DATABASE.PASSWORD",
        "SSLMode" : "Preferred"        
    },
    "EnableLogging": true,
    "reloadOnChange": false,
    "AccountServer": {
        "Endpoint": "ACCOUNTSERVER.URL",
        "ClientId": "BotServer",
        "ClientSecret": "ACCOUNTSERVER.CLIENTSECRET",
        "Scope" : "read write"
    }
}